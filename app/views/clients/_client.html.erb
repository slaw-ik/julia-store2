<div id="<%= dom_id client %>">
  <p>
    <strong>Nickname:</strong>
    <%= client.nickname %>
  </p>

  <p>
    <strong>First name:</strong>
    <%= client.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= client.last_name %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= client.address_id %>
  </p>

</div>
